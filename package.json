{
  "name": "firebase-firestore-presence",
  "version": "1.0.0",
  "scripts": {
    "pretest": "eslint --ext .js .",
    "test": ":",
    "build": "babel index.es2017.js -o index.js",
    "build:watch": "babel -w index.es2017.js -o index.js",
    "prestart": "firebase functions:config:get > .runtimeconfig.json && npm run build:watch &",
    "start": "firebase serve --only functions",
    "prestart:shell": "firebase functions:config:get > .runtimeconfig.json && npm run build:watch &",
    "start:shell": "firebase experimental:functions:shell",
    "predeploy": "npm run build",
    "deploy": "firebase deploy --only functions"
  },
  "dependencies": {
    "cors": "^2.8.4",
    "firebase-admin": "^5.5.0",
    "firebase-functions": "^0.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2017": "^6.24.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "firebase-tools": "^3.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RomansBermans/firebase-firestore-presence.git"
  },
  "bugs": {
    "url": "https://github.com/RomansBermans/firebase-firestore-presence/issues"
  },
  "homepage": "https://github.com/RomansBermans/firebase-firestore-presence",
  "author": "Romans Bermans <romans.bermans@gmail.com>",
  "license": "ISC"
}
